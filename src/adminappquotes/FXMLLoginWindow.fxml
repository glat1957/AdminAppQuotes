<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="adminappquotes.FXMLLoginWindowController">
   <children>
      <TextField fx:id="username" layoutX="67.0" layoutY="73.0" promptText="Username" />
      <Label layoutX="29.0" layoutY="26.0" text="Quotations Admin Utility Login">
         <font>
            <Font size="17.0" />
         </font>
      </Label>
      <Button fx:id="loginbutton" layoutX="182.0" layoutY="153.0" mnemonicParsing="false" onAction="#handleLogin" text="Login" />
      <Label fx:id="couldNotLogIn" alignment="CENTER_RIGHT" layoutX="10.0" layoutY="158.0" prefHeight="17.0" prefWidth="167.0" textFill="RED" />
      <PasswordField fx:id="password" layoutX="68.0" layoutY="108.0" promptText="Password" />
   </children>
</AnchorPane>
